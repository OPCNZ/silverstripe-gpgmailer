{
    "name": "opcnz/silverstripe-gpgmailer",
    "description": "The GPG Mailer module improves privacy by allowing emails being sent from SilverStripe CMS to be encrypted (and signed) using GPG public and private keys.",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "cwp",
        "email",
        "privacy",
        "GPG",
        "PGP",
        "OpenPGP"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Silverstripe Ltd.",
            "email": "info@silverstripe.com"
        }
    ],
    "homepage": "https://github.com/opcnz/silverstripe-gpgmailer",
    "support": {
        "issues": "https://github.com/opcnz/silverstripe-gpgmailer/issues",
        "source": "https://github.com/opcnz/silverstripe-gpgmailer/tree/master"
    },
    "require": {
        "nightjar/swiftsigner-cryptgpg": "^1",
        "silverstripe/framework": "^4"
    },
    "autoload": {
        "psr-4": {
            "OPCNZ\\GPGMailer\\": "src"
        }
    },
    "minimum-stability": "dev"
}
